
import android.content.Intent;
import android.support.v7.app.AppCompatActivity;
import android.os.Bundle;
import android.view.View;

 
public class MainActivity extends AppCompatActivity {

public void onButton3(View view){

        Intent fromMainToThree= new Intent(this ,Main24Activity.class);
        startActivity(fromMainToThree);
    }

    public void onButton4(View view){

        Intent fromMainToFour= new Intent(this ,Main23Activity.class);
        startActivity(fromMainToFour);
    }}
